{
  "defaultSeverity": "warning",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {

  },
  "rules": {
      "no-console": {
          "severity": "warning",
          "options": [
              "debug",
              "info",
              "log",
              "time",
              "timeEnd",
              "trace"
          ]
      },
      "typedef-whitespace": false,
      "align": [true, "parameters", "statements"],
      "whitespace": [true, "check-branch",
          "check-decl",
          "check-operator",
          "check-module",
          "check-separator",
          "check-rest-spread",
          "check-type",
          "check-typecast",
          "check-type-operator",
          "check-preblock"
      ],
      "no-trailing-whitespace": true,
      "import-spacing": false,
      "one-line": true,
      "eofline": true,
      "indent": [true, "spaces", 4],
      "linebreak-style": [false, "CRLF"],
      "prefer-const": true,
      "semicolon": true,
      "no-consecutive-blank-lines": [true, 1],
      "arrow-parens": [true],
      "space-within-parens": false,
      "max-line-length": false,
      "no-bitwise": false,
      "object-literal-shorthand": false,
      "only-arrow-functions": false
  },
  "rulesDirectory": []
}
